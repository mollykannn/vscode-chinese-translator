{
  "name": "vscode-chinese-translator",
  "displayName": "Chinese Translator",
  "description": "Translate between Traditional Chinese and Simplified Chinese.",
  "publisher": "mollykannn",
  "version": "1.1.0",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/mollykannn/vscode-chinese-translator.git"
  },
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "translator",
    "translation",
    "translate",
    "chinese",
    "traditional chinese",
    "simplified chinese"
  ],
  "activationEvents": [
    "onCommand:extension.translateToTraditional",
    "onCommand:extension.translateToSimplified"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.translateToSimplified",
        "title": "Translate traditional to simplified chinese"
      },
      {
        "command": "extension.translateToTraditional",
        "title": "Translate simplified to traditional Chinese"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^17.0.42",
    "@types/vscode": "^1.68.0",
    "eslint": "^8.17.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "vscode-test": "^1.3.0"
  }
}
